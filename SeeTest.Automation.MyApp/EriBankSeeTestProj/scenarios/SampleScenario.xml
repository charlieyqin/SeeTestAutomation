<?xml version="1.0" encoding="UTF-8"?><commands>
<command enable="true" name="SetDevice(Device)">
<parameters>
<parameter defaultValue="ios_app:Lab1" description="Device name" name="Device" pattern="(.+)" type="application"/>
</parameters>
</command>
<command enable="true" name="Launch(activity, instrument, stopIfRunning)">
<parameters>
<parameter defaultValue="com.experitest.ExperiBank" description="The application main activity or URL" name="Activity/URL" pattern="(.+)" type="launch"/>
<parameter defaultValue="true" description="If set to true then will launch in instrument mode" name="Instrument" pattern="(true|false)" type="bool"/>
<parameter defaultValue="false" description="If set to true then will stop the running process before launching it" name="Stop If Running" pattern="(true|false)" type="bool"/>
</parameters>
</command>
<command enable="true" name="IsElementFound(Zone, Element, Index)">
<parameters>
<parameter defaultValue="default" description="Select Zone" name="Zone" pattern="" type="zone"/>
<parameter defaultValue="EriBankLogo" description="Element to find" name="Element" pattern="" type="element"/>
<parameter defaultValue="0" description="Element index" name="Index" pattern="" type="integer"/>
</parameters>
</command>
</commands>
